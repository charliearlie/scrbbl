{
  "name": "Scrbbl",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "npm start --prefix client",
    "server": "node ./bin/www",
    "start": "npm run build",
    "prebuild": "cd client && npm i && npm run build",
    "build": "node ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "~4.15.5",
    "lastfmapi": "^0.1.1",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  }
}